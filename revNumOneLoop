package Other;

public class OtherPractice {

    public static int revNumOneLoop(int n){
        
        int temp = n;
        int reversed = 0;

        while (reversed < (n * 10)){
            reversed += temp % 10;
            reversed *= 10;
            temp /= 10;
        }

        return reversed / 10;
    }

    public static void main(String[] args) {
        int rev = revNumOneLoop(12345); //test case
        System.out.println(rev); //result
    }
}
